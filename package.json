{
  "name": "@iodigital/vite-plugin-msw",
  "description": "Mock Service Worker browser and node integration for Vite",
  "version": "1.0.0",
  "author": {
    "name": "iO",
    "website": "https://iodigital.com"
  },
  "repository": "https://github.com/iodigital-com/vite-plugin-msw",
  "keywords": [
    "msw",
    "vite",
    "mock service worker"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --watch",
    "build": "unbuild",
    "lint": "eslint --ext .js,.vue,.ts --fix --ignore-path .gitignore .",
    "test": "cross-env PW_EXPERIMENTAL_SERVICE_WORKER_NETWORK_EVENTS=1 playwright test -c tests/playwright.config.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mswjs/http-middleware": "^0.8.0",
    "body-parser": "^1.20.2",
    "fs-extra": "^11.1.1",
    "headers-polyfill": "^4.0.2",
    "strict-event-emitter": "^0.5.1"
  },
  "peerDependencies": {
    "msw": "^2.0.0"
  },
  "devDependencies": {
    "@mswjs/interceptors": "^0.25.7",
    "@playwright/test": "^1.39.0",
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/body-parser": "^1.19.4",
    "@types/connect": "^3.4.37",
    "@types/fs-extra": "^11.0.3",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "msw": "^2.0.0",
    "playwright": "^1.39.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.5.0"
  },
  "workspaces": [
    "examples/**"
  ]
}
