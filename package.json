{
  "name": "@iodigital/vite-plugin-msw",
  "description": "Mock Service Worker browser and node integration for Vite",
  "version": "0.3.0",
  "author": {
    "name": "iO",
    "website": "https://iodigital.com"
  },
  "repository": "https://github.com/iodigital-com/vite-plugin-msw",
  "keywords": [
    "msw",
    "vite",
    "mock service worker"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --watch",
    "build": "unbuild",
    "lint": "eslint --ext .js,.vue,.ts --fix --ignore-path .gitignore .",
    "test": "cross-env PW_EXPERIMENTAL_SERVICE_WORKER_NETWORK_EVENTS=1 playwright test -c tests/playwright.config.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mswjs/http-middleware": "^0.5.2",
    "body-parser": "^1.20.1",
    "fs-extra": "^10.1.0",
    "headers-polyfill": "^3.1.2",
    "strict-event-emitter": "^0.2.8"
  },
  "peerDependencies": {
    "msw": "^0.47.4"
  },
  "devDependencies": {
    "@mswjs/interceptors": "^0.17.6",
    "@playwright/test": "^1.32.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/body-parser": "^1.19.2",
    "@types/connect": "^3.4.35",
    "@types/fs-extra": "^9.0.13",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "msw": "^0.47.4",
    "playwright": "^1.32.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "unbuild": "^0.9.4",
    "vite": "^3.0.0"
  },
  "workspaces": [
    "examples/**"
  ]
}
